@model ShipperResponseModel
<h1>Are you sure you want to delete</h1>
<form asp-action="delete" asp-controller="Shipper">
<div>
    <label>Name</label>
    <input type="text" disabled value="@Model.Name" name="ShipperName"  class="form-control"/>
    <input type="text" disabled value="@Model.Price" name="ShipperPrice" class="form-control" />
    <input type="hidden" value="@Model.Id" name="Id" />
</div>
<button class="btn btn-danger">Delete</button>

</form>